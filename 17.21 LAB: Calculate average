import java.util.*;

public class LabProgram {
   
   public static int calcAverage(double[] doubleNums) {

      double doubleSum = 0;

      for (int i = 0; i < doubleNums.length; i++) {
         doubleSum = doubleSum + doubleNums[i];
      }

      double doubleAverage = doubleSum / doubleNums.length;
      int integerAverage = (int) doubleAverage;

      return integerAverage;

   }
   
   public static void main(String[] args) {

      double [] doubleNums = {1.1, 2.1, 3.1, 4.1, 5.1};
      System.out.println(calcAverage(doubleNums)); 

   }
}
