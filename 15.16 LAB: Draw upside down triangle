import java.util.Scanner;

public class LabProgram {
   public static void main(String[] args) {

      Scanner userInput = new Scanner(System.in);
      int arrowHeadWidth = userInput.nextInt();

      for (int i = arrowHeadWidth; i > 0; i--) {
         for (int j = 0; j < i; j++) {
            System.out.print("* ");
         }
         System.out.println();
      }

   }
}
