import java.util.Scanner;
import java.util.Arrays;

public class LabProgram {
   public static void main(String[] args) {
      
      Scanner userInput = new Scanner(System.in);

      char[] characterArray = new char[3];
      characterArray[0] = userInput.next().charAt(0);
      characterArray[1] = userInput.next().charAt(0);
      characterArray[2] = userInput.next().charAt(0);
      
      Arrays.sort(characterArray);
      
      System.out.println(characterArray[0] + " " + characterArray[1] + " " + characterArray[2]);

   }
}
